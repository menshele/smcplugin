          }
        case 49: break;
        case 20: 
          { return POP_KEYWORD;
          }
        case 50: break;
        case 8: 
          { return BRACKET_OPEN;
          }
        case 51: break;
        case 28: 
          { return START_KEYWORD;
          }
        case 52: break;
        case 17: 
          { return VERBATIM_OPEN;
          }
        case 53: break;
        case 15: 
          { return ASSIGN_OP;
          }
        case 54: break;
        case 13: 
          { return COMMA;
          }
        case 55: break;
        case 1: 
          { return RAW_CODE;
          }
        case 56: break;
        case 31: 
          { return IMPORT_KEYWORD;
          }
        case 57: break;
        case 5: 
          { return SLASH_SIGN;
          }
        case 58: break;
        case 32: 
          { return HEADER_KEYWORD;
          }
        case 59: break;
        case 12: 
          { return COLON;
          }
        case 60: break;
        case 4: 
          { return WORD;
          }
        case 61: break;
        case 25: 
          { return BLOCK_COMMENT;
          }
        case 62: break;
        case 23: 
          { return MAP_KEYWORD;
          }
        case 63: break;
        case 37: 
          { return FSM_CLASS_KEYWORD;
          }
        case 64: break;
        case 18: 
          { return VERBATIM_CLOSE;
          }
        case 65: break;
        case 24: 
          { return PUSH_KEYWORD;
          }
        case 66: break;
        case 33: 
          { return PACKAGE_KEYWORD;
          }
        case 67: break;
        case 26: 
          { return EXIT_KEYWORD;
          }
        case 68: break;
        case 22: 
          { return RAW_CODE_LINE;
          }
        case 69: break;
        case 21: 
          { return NIL_KEYWORD;
          }
        case 70: break;
        case 7: 
          { return BRACE_CLOSE;
          }
        case 71: break;
        case 2: 
          { return com.intellij.psi.TokenType.WHITE_SPACE;
          }
        case 72: break;
        case 9: 
          { return BRACKET_CLOSE;
          }
        case 73: break;
        case 30: 
          { return ACCESS_KEYWORD;
          }
        case 74: break;
        default:
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            return null;
          }
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
